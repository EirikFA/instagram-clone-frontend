{
  "name": "instagram-clone",
  "version": "0.0.0",
  "description": "Instagram clone frontend built using Ionic with React",
  "main": "src/index.tsx",
  "scripts": {
    "start": "craco start",
    "ionic:serve": "npm run start",
    "build": "npm run generate && craco build",
