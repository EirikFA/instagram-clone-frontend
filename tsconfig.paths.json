{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@api": [
        "api"
      ],
      "@api/*": [
        "api/*"
      ],
      "@components": [
        "components"
      ],
      "@components/*": [
        "components/*"
      ],
      "@generated": [
        "generated"
      ],
